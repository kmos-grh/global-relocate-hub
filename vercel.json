{
    "buildCommand": "npm run vercel-build",
    "outputDirectory": ".",
    "functions": {},
    "rewrites": [
        {
            "source": "/",
            "destination": "/index.html"
        },
        {
            "source": "/about",
            "destination": "/pages/about.html"
        },
        {
            "source": "/tools",
            "destination": "/pages/tools_calculators_hub.html"
        },
        {
            "source": "/guides",
            "destination": "/pages/guides.html"
        },
        {
            "source": "/contact",
            "destination": "/pages/contact.html"
        },
        {
            "source": "/partners",
            "destination": "/pages/partners_services_network.html"
        },
        {
            "source": "/routes",
            "destination": "/pages/routes_destinations_explorer.html"
        },
        {
            "source": "/blog",
            "destination": "/pages/blog_archive.html"
        },
        {
            "source": "/privacy",
            "destination": "/pages/privacy_policy.html"
        },
        {
            "source": "/terms",
            "destination": "/pages/terms_of_service.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        },
        {
            "source": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|woff|woff2)$)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}